@model List<contohaspcore.Models.Student>

@{
    Layout=null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student</title>
</head>
<body>
    <h2>Daftar Student</h2>
    <table border="1">
        <tr>
            <th>Nim</th><th>Nama</th><th>Alamat</th><th>Telp</th>
        </tr>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Nim</td>
                <td>@student.Nama</td>
                <td>@student.Alamat</td>
                <td>@student.Telp</td>
            </tr>
        }
    </table>
    
</body>
</html>